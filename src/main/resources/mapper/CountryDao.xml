<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.CountryDao">

    <select resultType="com.demo.entity.Country" id="findById" parameterType="Integer">
        select *
        from country where country_id = #{id}
    </select>

</mapper>